{
  "name": "teacher-portal-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f env/.env.development next dev",
    "build": "env-cmd -f env/.env.production next build",
    "build:demo": "env-cmd -f env/.env.demo next build",
    "build:staging": "env-cmd -f env/.env.staging next build",
    "start": "next start",
    "prepare": "husky install",
    "export": "env-cmd -f env/.env.production next build && next export",
    "export:demo": "env-cmd -f env/.env.demo next build && next export",
    "export:staging": "env-cmd -f env/.env.staging next build && next export",
    "lint:fix": "eslint --fix --ext .js,.jsx ."
  },
  "dependencies": {
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.58",
    "@rematch/core": "2.0.1",
    "@rematch/immer": "2.0.1",
    "@rematch/loading": "2.0.1",
    "axios": "0.21.1",
    "bootstrap": "^4.6.1",
    "classnames": "2.3.1",
    "crypto-js": "^4.1.1",
    "formik": "2.2.9",
    "googleapis": "^94.0.0",
    "http2": "^3.3.7",
    "immer": "9.0.3",
    "jquery": "^3.6.0",
    "js-cookie": "2.2.1",
    "jsrsasign": "^10.5.1",
    "jsrsasign-util": "^1.0.5",
    "moment": "^2.29.4",
    "next": "^10.1.3",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-bootstrap": "^2.4.0",
    "react-debounce-input": "^3.2.5",
    "react-doc-viewer": "^0.1.5",
    "react-dom": "17.0.2",
    "react-google-drive-picker": "^1.1.4",
    "react-hook-form": "^7.30.0",
    "react-html-parser": "^2.0.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-modal": "^3.15.1",
    "react-moment": "^1.1.2",
    "react-redux": "7.2.3",
    "react-show-more-text": "^1.5.2",
    "react-slick": "0.28.1",
    "react-toastify": "^8.2.0",
    "redux": "4.0.5",
    "sass": "1.32.11",
    "slick-carousel": "1.8.1",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "env-cmd": "10.1.0",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.23.2",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "2.2.1"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
